
sql = class( "funny.sql.Sql" )( "org.apache.derby.jdbc.EmbeddedDriver", "jdbc:derby:memory:test;create=true" );

sql.eachResult( "SELECT * FROM SYS.SYSTABLES", ( result ->
	println( result.getString( "TABLENAME" ) );
) );

tables = sql.list( "SELECT * FROM SYS.SYSTABLES" );
if( !tables; throw( "Expected some tables" ) );

tables.each( table -> println( table[ "tablename" ] ) );

tables.collect( t -> t[ "tablename" ] ).each( t -> println( t ) );
